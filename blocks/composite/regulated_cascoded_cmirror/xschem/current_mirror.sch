.option savecurrent
.control
save all

** Plot Vanilla Current Mirror
dc VOUT1 0 3.3 0.01

setplot dc1
let i_vanilla = viout1#branch
plot i_vanilla

dc VOUT2 0 3.3 0.01
setplot dc2
let i_biased = viout2#branch
plot i_biased

dc VOUT3 0 3.3 0.01 Iaux 50p 1n 100p
setplot dc3
let i_cascoded = viout3#branch
plot i_cascoded

write current_mirror.raw viout1#branch viout2#branch viout3#branch
.endc
"}
